GET    /assets/*file             controllers.Assets.at(path="/public", file)

# Login
POST   /login-byToken            controllers.Account.byToken

# Record posting
POST   /record/new-session       controllers.RecordSession.start
POST   /record/photo             controllers.RecordSession.addPhoto
POST   /record/submit            controllers.RecordSession.submit
POST   /record/publish           controllers.RecordSession.publish

# Record loading
POST   /record/load              controllers.Record.load
