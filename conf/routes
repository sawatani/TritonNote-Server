GET    /assets/*file             controllers.Assets.at(path="/public", file)

# Login
POST   /login-byToken            controllers.Account.byToken

# Record posting
POST   /record/new-session       controllers.CatchesSession.start
POST   /record/photo             controllers.CatchesSession.addPhoto
POST   /record/submit            controllers.CatchesSession.submit

# Record loading
POST   /record/load              controllers.RecordSession.load
